// ---------------------------------------------------------------------------------
//	¥ natApparentFree()
// ---------------------------------------------------------------------------------
// used as avl_free_key_fun_type
int natApparentFree (void * key) {
  NatEntry *entry;
  entry = (NatEntry *)key;
  [entry release];
  return 0;
}



// ---------------------------------------------------------------------------------
//	¥ natApparentCompare()
// ---------------------------------------------------------------------------------
// used as avl_key_compare_fun_type
int natApparentCompare (void * compare_arg, void * a, void * b)
{
  NatEntry *ta;
  NatEntry *tb;
  ta = (NatEntry *)a;
  tb = (NatEntry *)b;
  
  return [ta compare:tb];
}


