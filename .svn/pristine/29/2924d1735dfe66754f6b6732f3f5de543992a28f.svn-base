.none{ /* fixes a netscape4 issue */}body,div,p,th,td,ol,ul,li {	font-family: Verdana, Arial, Helvetica,sans-serif;	font-size: 13px;	color: #333333;}a,a:visited,a:active,a:link {	color: #666699;	text-decoration: none;}a:hover {	text-decoration: underline;	color: #999999;}h2{	font-family: Verdana, Arial, Helvetica, sans-serif;	font-size: 14px;	font-weight: bold;}h2 {	color: #666699;	margin-bottom: 4px;}h3 {	margin-bottom: 2px;}h4 {	font-family: Verdana, Arial, Helvetica, sans-serif;	font-size: 14px;	color: #666699;}li {font-family: Verdana, Arial, Helvetica, sans-serif;font-size: 13px;list-style-position: outside;list-style-type: square;color: #333333;white-space: normal;line-height: 130%;}pre {	font-family: "Courier New", Courier, monospace;	font-size: 12px;	whitespace: pre;}a.whitelink {color: #ffffff;text-decoration: none;}a.whitelink:hover {text-decoration: underline;color: #ffffff;}.body {	font-family: Verdana, Arial, sans serif;	font-size: 13px;	color: #333333;}.bodyWhite {	font-family: Verdana, Arial, sans serif;	font-size: 14px;	color: #ffffff;}.bodyLink {	font-family: Verdana, Arial, sans serif;	font-size: 14px;	color: #003333;	font-weight: bold;}.listLink {	font-family: Verdana, Arial, sans serif;	font-size: 14px;	color: transparent;	display: block;	margin-bottom: 6px;}.bodyNav {	font-family: Verdana, Arial, sans serif;	font-size: 13px;	color: #000000;	text-decoration: none;}.bodyNavOn {	font-family: Verdana, Arial, sans serif;	font-size: 13px;	color: transparent;	text-decoration: none;}.header {	font-family: Verdana, Arial, sans serif;	font-size: 14px;	font-weight: bold;	color: #666666;	font-style: italic;	margin-bottom: 4px;}.subHeader {	font-family: Verdana, Arial, sans serif;	font-size: 14px;	font-weight: bold;	margin-bottom: 2px;	color: #000000;}.auxNav {	font-family: Verdana, Arial, sans serif;	font-size: 13px;	color: transparent;	text-decoration: none;}.auxNav:hover {	text-decoration: underline;}.copyright {	font-family: Verdana, Arial, "sans serif";	font-size: 13px;	color: #ffffff;	text-decoration: none;	background-color: 663399;}.tableData {	font-family: Arial, sans serif;	font-size: 13px;	color: #000000;}.navigation {  font-family: Arial, Helvetica, sans-serif; font-size: 11px}tt {  font-family: "Courier New", Courier, mono; font-size: 11px; color: #000000}h3  { 	font-family: Verdana, Arial, Helvetica, sans-serif;	font-size: 13px;	font-weight: bold; } 